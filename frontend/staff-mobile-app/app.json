{
  "expo": {
    "name": "apricot&nut(staff)",
    "slug": "staff-mobile-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/apricot-nut-logo-word.png",
    "splash": {
      "image": "./assets/images/apricot-nut-logo-word.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rt04capstone.apricotstaff",
      "permissions": ["CAMERA"],
      "googleServicesFile": "./google-services.json"
    }
  }
}
